{
    "name": "ziiframework/debug",
    "description": "zii debug",
    "keywords": [
        "zii",
        "debug"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziiframework/debug/issues",
        "source": "https://github.com/ziiframework/debug"
    },
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "http://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*",
        "ziiframework/zii": "3.0.x-dev",
        "opis/closure": "^3.3"
    },
    "require-dev": {
        "ziiframework/swiftmailer": "3.0.x-dev",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "yii\\debug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\debug\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.cn"
        }
    ]
}
