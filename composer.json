{
    "name": "ziiframework/debug",
    "description": "zii-framework debug",
    "keywords": [
        "zii-framework",
        "debug"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziiframework/debug/issues",
        "source": "https://github.com/ziiframework/debug"
    },
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "http://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ext-mbstring": "*",
        "ziiframework/zii": "~3.1.1",
        "opis/closure": "^3.3"
    },
    "require-dev": {
        "ziiframework/swiftmailer": "~3.2.2",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "yii\\debug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\debug\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.cn"
        }
    ],
    "scripts": {
        "redo-install": [
            "rm -rf vendor/",
            "rm -f composer.lock",
            "@composer self-update -vvv",
            "@composer install -vvv"
        ]
    }
}
